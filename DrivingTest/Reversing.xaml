<UserControl x:Class="DrivingTest.Reversing"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Height="600" Width="800">
    <Grid>
        <Image Height="295" HorizontalAlignment="Left" Margin="12,15,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="423" Source="/DrivingTest;component/Images/倒车入库01.jpg" />
        <TextBlock Height="292" HorizontalAlignment="Left" Margin="460,18,0,0" Name="textBlock1" Text="考试项目： 倒车入库 
操作要求： 考试过程中，车辆进退途中不得停车。从道路一端控制线（车身压控制线）倒入车库停车，再前进出库向另一端驶过控制线后倒入车库停车，最后前进驶出车库。 
考试办法： 下列情况之一为不合格

不按规定路线、顺序行驶的，不合格； 
车身出线的，不合格； 
倒库不入的，不合格； 
中途停车的，不合格。 
" VerticalAlignment="Top" Width="316" TextWrapping="Wrap" />
        <Button Content="返回" Height="23" HorizontalAlignment="Left" Margin="333,437,0,0" Name="fanhui_button" VerticalAlignment="Top" Width="75" Click="fanhui_button_Click" />
    </Grid>
</UserControl>
